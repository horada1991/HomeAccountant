<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="HomeAccountant"
                   namespace="HomeAccountant.Model.Domain">

  <class name="Account">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="AvailableFunds"/>
  
    <one-to-one name="UserData" class="UserData"/>

    <set name="SavingsTypes" table="Account_SavingsType" cascade="all">
      <key column="AccountId" />
      <many-to-many column="SavingsTypesId" unique="true" class="SavingsType" />
    </set>
  </class>

</hibernate-mapping>